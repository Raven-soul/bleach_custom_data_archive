{
    "templates":[
        {
            "Id":0,
            "Name": "mainMenuTableRow",
            "ChevronClassName": "menu-section-chevron-data",
            "Function": "hideChangeChevron('@@BLOCKLISTCLASSNAME@@', '@@BLOCKCHEVRONCLASSNAME@@', this)",
            "data": "<div class=\"col main-menu-block-item\"><button onclick=\"@@BLOCKFUNCTION@@\" id=\"@@BLOCKID@@\" class=\"col w-100 px-3 main-menu-block-item-label\"><div class=\"row d-flex justify-content-between\"><div class=\"col label\">@@BLOCKNAME@@</div><div class=\"col-auto d-flex align-items-center @@BLOCKCHEVRONCLASSNAME@@-@@BLOCKID@@\"><i class=\"fa-solid fa-chevron-down\"></i></div></div></button><div class=\"col main-menu-block-item-list @@BLOCKLISTCLASSNAME@@-@@BLOCKID@@\"><ul>@@BLOCKLISTDATA@@</ul></div></div></div></div></div>"
        },
        {
            "Id":1,
            "Name": "mainMenuTableRowListItem",
            "data": "<li><button onclick=\"@@LIROWFUNCTION@@\" class=\"w-100 p-0\"><div class=\"row m-0\"><div class=\"col-1 p-0 image-data\">@@LIROWICO@@</div><div class=\"col content-data\">@@LIROWNAME@@</div></div></button></li>"
        }
    ]
}
                   