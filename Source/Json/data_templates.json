{
    "templates":[
        {
            "Id":0,
            "Name":"Main menu template",
            "segments":[
                {
                    "Id":0,
                    "Name": "mainMenuTableRow",
                    "ChevronClassName": "menu-section-chevron-data",
                    "Function": "hideChangeChevron('@@BLOCKLISTCLASSNAME@@', '@@BLOCKCHEVRONCLASSNAME@@', this)",
                    "data": "<div class=\"col main-menu-block-item\"><button onclick=\"@@BLOCKFUNCTION@@\" id=\"@@BLOCKID@@\" class=\"col w-100 px-3 main-menu-block-item-label\"><div class=\"row d-flex justify-content-between\"><div class=\"col label\">@@BLOCKNAME@@</div><div class=\"col-auto d-flex align-items-center @@BLOCKCHEVRONCLASSNAME@@-@@BLOCKID@@\"><i class=\"fa-solid fa-chevron-down\"></i></div></div></button><div class=\"col main-menu-block-item-list @@BLOCKLISTCLASSNAME@@-@@BLOCKID@@\"><ul>@@BLOCKLISTDATA@@</ul></div></div></div></div></div>"
                },
                {
                    "Id":1,
                    "Name": "mainMenuTableRowListItem",
                    "data": "<li><button onclick=\"@@LIROWFUNCTION@@\" class=\"w-100 p-0\"><div class=\"row m-0\"><div class=\"col-1 p-0 image-data\">@@LIROWICO@@</div><div class=\"col content-data\">@@LIROWNAME@@</div></div></button></li>"
                }
            ]
        },
        {
            "Id":1,
            "Name":"Main info block template",
            "segments":[
                {
                    "Id":0,
                    "Name": "mainInfoBlockNewsType",
                    "data": "<div class=\"col  main-info-block-item\"><div class=\"row-2\"><div class=\"col news-info-block-header\">@@INFOBLOCKNAME@@</div><div class=\"col news-info-block-body\"><div class=\"row-2\"><div class=\"col\">@@INFOBLOCKDATA@@</div><div class=\"col image-data\"><img src=\"@@INFOBOCKIMAGE@@\"></div><div class=\"col\"><button class=\"p-0\" onclick=\"@@INFOBLOCKFUNCTION@@\">Читать дальше</button></div></div></div></div></div>"
                },
                {
                    "Id":1,
                    "Name": "mainInfoBlockClassType",
                    "data": "<div class=\"col  main-info-block-item\"><div class=\"row-2\"><div class=\"col news-info-block-header\">@@INFOBLOCKNAME@@</div><div class=\"col news-info-block-body\"><div class=\"row-2\"><div class=\"col\">@@INFOBLOCKDATA@@</div><div class=\"col image-data\"><img src=\"@@INFOBOCKIMAGE@@\"></div><div class=\"col\"><button class=\"p-0\" onclick=\"@@INFOBLOCKFUNCTION@@\">Читать дальше</button></div></div></div></div></div>"
                }
            ]
        },
        {
            "Id":2,
            "Name":"Main classes block template",
            "segments":[
                {
                    "Id":0,
                    "Name": "mainInfoBlockClassType",
                    "data": "<div class=\"row-2\"><div class=\"col\"><div class=\"grid-data\">@@INFOBLOCKGRIDDATA@@</div></div></div>"
                },
                {
                    "Id":1,
                    "Name": "mainInfoBlockClassItemType",
                    "data": "<div class=\"class-block-item\"><div class=\"main-info-block-item\" onclick=\"@@INFOBLOCKCLASSFUNCTION@@\"><img src=\"@@INFOBLOCKCLASSIMAGE@@\"><div class=\"row-2 ps-2\"><div class=\"col class-name-head\">@@INFOBLOCKCLASSHEADNAME@@</div><div class=\"col class-name-append\">@@INFOBLOCKCLASSAPPENDNAME@@</div></div></div></div>"
                }
            ]
        }
    ]
}
                   